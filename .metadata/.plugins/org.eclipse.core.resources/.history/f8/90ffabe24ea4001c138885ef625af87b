import java.util.Comparator;

public class myPair {
	private int Freq;
	private char C;
	myPair(int freq, char c){
		Freq = freq;
		C = c;
	}

	static class ComPair implements Comparator<myPair>{

		@Override
		public int compare(myPair p1, myPair p2) {
			
			return p1.Freq.compareTo(p2.Freq);
		}
		
	}

	
}
