import java.util.Comparator;

public class myPair {
	private int Freq;
	private char C;
	myPair(int freq, char c){
		Freq = freq;
		C = c;
	}
	int getFreq(){
		return Freq;
	}
	char getChar(){
		return C;
	}
	static class ComPair implements Comparator<myPair>{

		@Override
		public int compare(myPair p1, myPair p2) {
			
			return p1.getFreq().compareTo(p2.getFreq());
		}
		
	}

	
}
